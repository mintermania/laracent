{
    "name": "mintermania/laracent",
    "type": "library",
    "description": "Centrifugo broadcaster for laravel 6 > and Centrifugo >= 2.1.0 (forked)",
    "keywords": ["laravel", "centrifugo", "broadcaster"],
    "license": "MIT",
    "authors": [
        {
            "name": "MinterMania",
            "email": "mintermania@gmail.com"
        }
    ],
    "homepage": "https://github.com/mintermania/laracent",
    "support": {
        "issues": "https://github.com/mintermania/laracent/issues",
        "source": "https://github.com/mintermania/laracent"
    },
    "require": {
        "php": ">=7.2.12",
        "laravel/framework": "^6.0.4",
        "guzzlehttp/guzzle": "^6.3.3",
        "illuminate/broadcasting": "^6.0.4"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7||^8"
    },
    "autoload": {
        "psr-4": {
            "AlexHnydiuk\\Laracent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexHnydiuk\\Laracent\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexHnydiuk\\Laracent\\LaracentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
